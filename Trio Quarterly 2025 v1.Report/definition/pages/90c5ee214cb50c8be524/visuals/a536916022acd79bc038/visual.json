                "textRuns": [
                  {
                    "value": "This report includes the annualized amount of any salary increase awarded during the selected report time period. ",
                    "textStyle": {
                      "fontSize": "9pt",
                      "color": "#000000"
                    }
                  },
                  {
                    "value": "Only benefit eligible part-time and full-time faculty and staff are included in the report totals",
