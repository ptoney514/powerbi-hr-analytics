table '(Terms) Terminations'
	lineageTag: 07e6f7ce-dcf2-4aee-885d-eed31f2c8114

	measure PercentTurnover_ByEmployeeType_FY = ```
			
			VAR FiscalStart = DATE(YEAR(TODAY()) - IF(MONTH(TODAY()) < 7, 1, 0), 7, 1)
			VAR FiscalEnd = DATE(YEAR(TODAY()) - IF(MONTH(TODAY()) < 7, 1, 0) + 1, 6, 30)
			
			VAR TermCount = 
			    CALCULATE(
			        COUNT('(Terms) Employee Details'[Empl Num]),
			        '(Terms) Terminations'[Term Date] <> BLANK(),
			        '(Terms) Terminations'[Term Date] >= FiscalStart,
			        '(Terms) Terminations'[Term Date] <= FiscalEnd
			    )
			
			VAR EmployeeTypeCount = 
			    CALCULATE(
			        DISTINCTCOUNT('(Active) Employee Details'[Empl num]),
			        'New Emp List SInce FY20'[Current Hire Date] <= FiscalEnd,
			        OR('(Terms) Terminations'[Term Date] > FiscalEnd, '(Terms) Terminations'[Term Date] = BLANK()),
			        '(Active) Job Details'[Person Type] = SELECTEDVALUE('(Active) Job Details'[Person Type], "Staff")
			    )
			
			RETURN
			    IF(EmployeeTypeCount > 0, DIVIDE(TermCount, EmployeeTypeCount), BLANK())
			```
		formatString: 0.00
		lineageTag: 450aeb8b-0f5c-4f5f-8362-97c8312c1160

